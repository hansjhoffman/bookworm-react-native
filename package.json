{
  "name": "bookworm",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:android": "cd android && bundle exec fastlane deploy",
    "build:ios": "cd ios && bundle exec fastlane deploy",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "run:android": "react-native run-android",
    "run:ios": "react-native run-ios",
    "schema": "graphql get-schema && yarn run codegen",
    "start": "react-native start",
    "test": "jest",
    "test:ci": "jest --ci --coverage"
  },
  "husky": {
    "hooks": {
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1 ]]; then yarn install --frozen-lockfile; fi",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rebase": "yarn install",
      "pre-commit": "yarn tsc && yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix", "yarn format", "git add"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.61.0-rc.3",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^2.0.0-alpha.1",
    "react-navigation": "^4.0.2",
    "react-navigation-drawer": "^2.1.1",
    "react-navigation-stack": "^1.5.4"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.0",
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/introspection": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@testing-library/jest-native": "^3.0.2",
    "@testing-library/react-native": "4.0.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.13",
    "@types/react-test-renderer": "^16.9.0",
    "apollo-link": "^1.2.13",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "graphql-cli": "^3.0.14",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jetifier": "^1.6.4",
    "lint-staged": "^9.2.5",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.6.2"
  }
}
